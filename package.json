{
  "name": "@penggy/pack",
  "version": "1.0.8",
  "description": "pack your nodejs project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "pack": "bin/pack.js"
  },
  "files": [
    "bin",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "zip",
    "tar",
    "package",
    "archive",
    "nodejs",
    "project"
  ],
  "author": "penggy",
  "license": "ISC",
  "dependencies": {
    "archiver": "2.1.1",
    "commander": "2.14.0",
    "fs-extra": "5.0.0",
    "glob": "7.1.2",
    "moment": "2.20.1",
    "pretty-bytes": "4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/penggy/pack"
  },
  "pack": {
    "pattern": "**",
    "options": {
      "dot": false,
      "ignore": ["node_modules/**", "*.log", "*.tar.gz", "*.zip"]
    }
  }
}
